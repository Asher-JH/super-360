{
  "name": "super-360",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npx tsc && npm run copy:public && npm run copy:views",
    "copy:public": "copyfiles -u 1 './src/public/**' './dist'",
    "copy:views": "copyfiles -u 1 './src/views/**' './dist'",
    "start": "node ./dist/server.js",
    "tailwind:css": "postcss src/public/stylesheets/tailwind.css -o src/public/stylesheets/styles.css",
    "dev": "nodemon --exec 'yarn run tailwind:css && nodemon ./src/server.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asher-JH/super-360.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Asher-JH/super-360/issues"
  },
  "homepage": "https://github.com/Asher-JH/super-360#readme",
  "dependencies": {
    "autoprefixer": "^10.4.4",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "sib-api-v3-sdk": "^8.3.0",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "nodemon": "^2.0.15",
    "prettier": "2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
